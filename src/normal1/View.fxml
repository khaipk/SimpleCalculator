<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="normal1.ViewController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" />
            <TextField alignment="CENTER" prefHeight="40.0" promptText="Fullname">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField alignment="CENTER" prefHeight="40.0" promptText="Email">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField alignment="CENTER" prefHeight="40.0" promptText="Username">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <PasswordField alignment="CENTER" prefHeight="40.0" promptText="Password">
               <font>
                  <Font size="18.0" />
               </font>
            </PasswordField>
            <PasswordField alignment="CENTER" prefHeight="40.0" promptText="Re-Password">
               <font>
                  <Font size="18.0" />
               </font>
            </PasswordField>
            <Region prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="signUp" alignment="CENTER" mnemonicParsing="false" onAction="#signUp" prefHeight="40.0" text="Sign Up">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label prefHeight="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
